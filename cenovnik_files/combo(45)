"use strict";YUI.add("tictac-smartviews-onboarding",function(a){var b=a.Tictac.smartviews.rollups;a.Tictac.smartviews.onboarding={renderSmartviewOnboarding:function(){(a.common.Utils.features.enabled("smartviewsOnboarding")||-1!==window.location.search.indexOf("smartviewsTutorial")&&NeoConfig.isInternal)&&a.use("tictac-base-mailpreferences",function(a){var c=window.NeoConfig,d=a.Tictac.base.mailpreferences,e=d.get("hasShownSmartviewsOnboarding"),f=function(b){yui.one("#smartviewsOnboarding").contains(b.target)||(a.fire("onboard:tipclose"),window.removeEventListener("click",f,!0),window.removeEventListener("keydown",f,!0),a.common.Utils.features.enabled("scout")&&yui.fire("callout:action",{name:"smartviewsOnboarding",action:"close"}))};(-1===window.location.href.indexOf("smartviewsTutorial")||!NeoConfig.isInternal)&&(e||c.showTabsCallout||c.showNavCallout||c.showOlympicCallout||c.onboardPC||c.isRegOnboardingEnabled||c.showingEnhancrTooltip)||(a.use("mail-ui-onboarding","comms-ui-tooltips-widget",function(a){d.set("hasShownSmartviewsOnboarding",!0),d.save(),c.showingSmartviewTooltip=!0,a.fire("onboard:tip",{node:a.one("#smartviews"),template:b.smartviews_onboarding(),fn:function(b){b.target.hasClass("btn")&&(a.fire("onboard:tipclose"),window.removeEventListener("click",f,!0),window.removeEventListener("keydown",f,!0))},dir:"left",width:265,offset:[120,-10]}),a.common.Utils.features.enabled("scout")&&(yui.fire("callout:action",{name:"smartviewsOnboarding",action:"shown",keyValue:1}),yui.on("closeCallout:smartviewsOnboarding",function(){yui.fire("onboard:tipclose"),window.removeEventListener("click",f,!0),window.removeEventListener("keydown",f,!0)}))}),window.addEventListener("click",f,!0),window.addEventListener("keydown",f,!0))})}}},"1.0.0",{requires:["tictac-smartviews-rollups","mail-common-utils-features"]});